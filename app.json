{
  "expo": {
    "name": "router-spotify",
    "slug": "router-spotify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "routerspotify",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.simek.routerspotify"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "dev.simek.routerspotify",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Gotham-Bold.otf",
            "./assets/fonts/Gotham-Medium.otf",
            "./assets/fonts/Gotham-Light.otf"
          ]
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
        [
          "expo-splash-screen",
          {
            "backgroundColor": "#000000",
            "image": "./assets/images/adaptive-icon.png",
            "dark": {
              "image": "./assets/images/adaptive-icon.png",
              "backgroundColor": "#000000"
            },
            "imageWidth": 256
          }
        ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6437c6f6-082a-4a82-a844-12db18839871"
      }
    },
    "owner": "simek"
  }
}
